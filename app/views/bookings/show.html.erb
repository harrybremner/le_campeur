  <% @campervan.bookings.each do |booking| %>
    <li> <%= booking.start_date %>
    <br>
    <%= booking.end_date %></p>
    <% if @campervan.photo.attached? %>
      <%= cl_image_tag @campervan.photo.key, height: 150, width: 200, crop: :fill %>
    <% end %>
    <br>
      <%= link_to "Delete",
    campervan_booking_path(booking) ,
    data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}
  %>
  <% end %>
